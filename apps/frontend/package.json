{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --fix \"./src/**/*.{ts,astro,svelte}\" && prettier -w --ignore-unknown \"./src/**/*\"",
    "check": "astro check src/ && svelte-check",
    "affected": "echo \"is-affected:$(jq -r '.name' package.json)\""
  },
  "dependencies": {
    "@astrojs/svelte": "^5.0.1",
    "@msgpack/msgpack": "3.0.0-beta2",
    "astro": "^4.0.4",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@fontsource-variable/dm-sans": "^5.0.3",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/mingcute": "^1.1.10",
    "@iconify-json/simple-icons": "^1.1.72",
    "@iconify-json/skill-icons": "^1.1.3",
    "@tailwindcss/typography": "^0.5.10",
    "astro-seo": "^0.8.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "eslint-config-custom": "workspace:^",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-svelte": "^2.34.0",
    "postcss": "^8.4.30",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "sharp": "^0.32.6",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tsconfig": "workspace:^",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.17.0"
  },
  "lint-staged": {
    "*.{astro,js,ts,mjs,cjs,svelte}": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}

{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "dotEnv": [".env.production", ".env.production.local", ".env"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development", ".env.development.local", ".env"],
      "env": ["HOME"]
    },
    "check": {},
    "genpasswd": {
      "cache": false
    },
    "affected": {
      "cache": false
    },
    "dagger": {
      "cache": false
    }
  },
  "globalPassThroughEnv": [
    "KUBE_CLUSTER_HOST",
    "KUBE_CA_DATA",
    "KUBE_CERT_DATA",
    "KUBE_KEY_DATA",
    "CI_PIPELINE_SOURCE",
    "CI_MERGE_REQUEST_TARGET_BRANCH_NAME",
    "CI_MERGE_REQUEST_SOURCE_BRANCH_NAME",
    "CI_PROJECT_PATH",
    "CI_COMMIT_REF_NAME",
    "CI_PIPELINE_SOURCE",
    "GITLAB_TOKEN",
    "DOCKER_HOST",
    "DOCKER_TLS_VERIFY",
    "DOCKER_TLS_CERTDIR",
    "DOCKER_CERT_PATH",
    "DOCKER_DRIVER",
    "DOCKER_VERSION",
    "CI"
  ]
}

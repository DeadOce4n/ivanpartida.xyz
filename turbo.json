{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "dotEnv": [".env.production", ".env.production.local", ".env"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development", ".env.development.local", ".env"],
      "env": ["HOME"]
    },
    "check": {},
    "genpasswd": {
      "cache": false
    }
  }
}
